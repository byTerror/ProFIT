    <validationRules>
        <fullName>EventDateTimeRule</fullName>
        <active>true</active>
        <description>End Date/Time must be at least 1 day ahead of Start Date/Time (If there is a value in End Date/Time field).</description>
        <errorConditionFormula>AND( NOT( ISBLANK( End_Date_Time__c ) ) , DATEVALUE(End_Date_Time__c) &lt;  DATEVALUE(Start_Date_Time__c)+1  )</errorConditionFormula>
        <errorDisplayField>End_Date_Time__c</errorDisplayField>
        <errorMessage>Duration of event must be at least 1 day</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventNotRecurringRule</fullName>
        <active>true</active>
        <description>If recurring is unchecked, user should not fill the Frequency field.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(TEXT( Frequency__c ))),   NOT(Recurring__c  ) )</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>You must not fill the frequency field for recurring checkbox is unchecked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventRecurringRule</fullName>
        <active>true</active>
        <description>If recurring is checked, user needs to fill the Frequency field.</description>
        <errorConditionFormula>AND(ISBLANK(TEXT( Frequency__c )),  Recurring__c )</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>You must fill the frequency field for recurring checkbox is checked</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventTypeInPersonRule</fullName>
        <active>true</active>
        <description>If event type is in-person, user must select location of event</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;In-Person&apos;) , ISBLANK( Location__c  ))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>You must select a location for event type is in-person</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>EventTypeVirtualRule</fullName>
        <active>true</active>
        <description>If event type is virtual, user is prevented to select location of event</description>
        <errorConditionFormula>AND( ISPICKVAL( Event_Type__c , &apos;Virtual&apos;) , NOT( ISBLANK( Location__c )))</errorConditionFormula>
        <errorDisplayField>Location__c</errorDisplayField>
        <errorMessage>You cannot pick a location for the event type is virtual</errorMessage>
    </validationRules>
